package baekjoon.chpter3;

import java.util.Scanner;

public class Ex_A_2739 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		
		for(int i = 1; i < 10; i++) {
			for(int j = 1; j < 10; j++) {
				System.out.println(a + "*" + j + "=" + a * j );
			}
		}
	

	}

}
